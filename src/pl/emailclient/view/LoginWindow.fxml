                layoutY="190.0"
                mnemonicParsing="false"
                onAction="#loginButtonAction"
                prefHeight="39.0"
                prefWidth="100.0"
                text="Login"/>
        <Label layoutX="42.0"
               layoutY="70.0"
               prefHeight="17.0"
               prefWidth="100.0"
               text="Email Address"/>
        <Label layoutX="42.0"
               layoutY="111.0"
               prefHeight="17.0"
               prefWidth="100.0"
               text="Password"/>
        <TextField fx:id="emailAddressField"
                   layoutX="163.0"
                   layoutY="66.0"
                   prefHeight="23.0"
                   prefWidth="285.0"/>
        <PasswordField fx:id="passwordField"
                       layoutX="163.0"
                       layoutY="107.0"
                       prefHeight="25.0"
                       prefWidth="285.0"/>
        <Label fx:id="errorLabel"
               layoutX="132.0"
               layoutY="257.0"
               prefHeight="25.0"
               prefWidth="210.0"/>
    </children>
</AnchorPane>
